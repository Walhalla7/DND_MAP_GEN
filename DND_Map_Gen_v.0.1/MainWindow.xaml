<Window x:Class="DND_Map_Gen_v._0._1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DND_Map_Gen_v._0._1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" InputScope="Url">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Label FontSize="50" FontWeight="Bold">
                MAP GENERATOR
            </Label>
            <Button VerticalAlignment="Center" Padding="10" HorizontalAlignment="Center" Click="SwitchToMap_Click">
                <TextBlock Margin="10,0">Generate Map</TextBlock>
            </Button>
        </StackPanel>
    </Grid>
    
</Window>
